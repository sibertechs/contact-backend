{
    "version": 2,
    "builds": [
        {
            "src": "api/contact.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/health.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/contact",
            "methods": ["POST", "OPTIONS"],
            "dest": "/api/contact.js"
        },
        {
            "src": "/api/health",
            "methods": ["GET"],
            "dest": "/api/health.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}